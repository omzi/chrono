  "homepage": "https://github.com/omzi/chrono#readme",
  "devDependencies": {
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "autoprefixer": "10.4.5",
    "css-loader": "6.7.1",
    "eslint": "7.32.0",
    "eslint-config-eslint": "7.0.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.5.0",
